fun main() {
 
    var mensagem: String? = null
    
    mensagem = "Mensagem correta"
    
    println("${mensagem ?:"Mensagem genérica"}")
    
    	if (mensagem != null){
            
            println("$mensagem")
            
        }
        
        	else{
                println("Mensagem genérica")
            }
    
    
    
}
